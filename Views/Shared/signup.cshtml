        function t(val)
        {
            document.getElementById("token").value=val;
            console.log(val);
        }
        jQuery("#txtirid").change(function(){
            if (jQuery("#txtirid").val().length == 8)
            {
                jQuery.get("https://vregistration.the-v.net/checkir.aspx?irid=" + jQuery("#txtirid").val(), function(data, status){
